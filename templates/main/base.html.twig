<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet"
              href="{{ asset('assets/framework/fontawesome-free-5.15.2-web/fontawesome-free-5.15.2-web/css/all.css') }}">
        <link rel="stylesheet"
              href="{{ asset('assets/framework/bootstrap-4.6.0-dist/bootstrap-4.6.0-dist/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/main/css/style.css') }}">
    {% endblock %}
</head>
<body>
{% block navbar %}
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="{{ path('home') }}">
            <i class="fas fa-cannabis"></i>
        </a>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="{{ path('home') }}">Home</a>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                <a href="{{ path('app_login') }}" class="btn btn-outline-success my-2 my-sm-0" type="login">Login</a>
            </form>
        </div>
    </nav>
{% endblock %}



{% block body %}{% endblock %}

{% block javascripts %}
    <script
            src="https://code.jquery.com/jquery-3.5.1.min.js"
            integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.6.0/umd/popper.min.js"
            integrity="sha512-BmM0/BQlqh02wuK5Gz9yrbe7VyIVwOzD1o40yi1IsTjriX/NGF37NyXHfmFzIlMmoSIBXgqDiG1VNU6kB5dBbA=="
            crossorigin="anonymous"></script>
    <script src="{{ asset('assets/framework/bootstrap-4.6.0-dist/bootstrap-4.6.0-dist/js/bootstrap.min.js') }}"></script>
{% endblock %}
</body>
</html>
